{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local","**/.env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "_next/**", "!_next/cache/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
